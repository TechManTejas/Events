    <div class="carousel-container">
      {% for event in events %}
        {{ include("hive:event-card", event) }}
      {% endfor %}
    </div>
    <button class="carousel-next">›</button>
  </div>
</div>

<script src="/themes/hive/components/events-card-list/events-card-list.js"></script>
